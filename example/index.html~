    <script type="text/javascript">
      scores = [30,30,30,30,30];
      labels = ["うんのよさ", "たいりょく", "ちから", "かしこさ", "すばやさ"];
      ids    = ["l", "s", "p", "c", "q"];
      //rader( "chart", 250, 250, scores, labels, null, 50);
      rader( "chart", 250, 250, scores, labels, ids, 50);
